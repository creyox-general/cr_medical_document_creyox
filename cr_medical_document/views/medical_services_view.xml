<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="medical_services_view_tree" model="ir.ui.view">
        <field name="name">medical.services.view.tree</field>
        <field name="model">medical.services</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
            </list>
        </field>
    </record>

    <record id="action_medical_services" model="ir.actions.act_window">
        <field name="name">Medical Services</field>
        <field name="res_model">medical.services</field>
        <field name="view_mode">list,form</field>
    </record>

    <menuitem name="Medical Services"
              id="medical_service_menu"
              parent="contacts.menu_contacts"
              action="action_medical_services"
              sequence="21"/>

</odoo>