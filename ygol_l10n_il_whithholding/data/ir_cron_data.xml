<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ir_cron_calculate_withholding_amount" model="ir.cron">
        <field name="name">Calculate Withholding Amount</field>
        <field name="model_id" ref="account.model_account_payment"/>
        <field name="state">code</field>
        <field name="code">model._cron_calculate_withholding_amount()</field>
        <field name='interval_number'>1</field>
        <field name='interval_type'>days</field>
        <field name="active">False</field>
    </record>
    <record id="cron_update_amount_after_withholding" model="ir.cron">
        <field name="name">Update Amount After Withholding</field>
        <field name="model_id" ref="model_account_payment"/>
        <field name="state">code</field>
        <field name="code">model.update_amount_after_withholding_cron()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="active" eval="False"/>
    </record>
</odoo>
