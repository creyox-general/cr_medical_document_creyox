<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_move_form_inherit_form" model="ir.ui.view">
        <field name="name">account.move.inherit.form</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='invoice_date']" position="after">
                <field name="salary"/>
            </xpath>
        </field>
    </record>

    <record id="view_company_inherit_form" model="ir.ui.view">
        <field name="name">view.res.company.inherit.form</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='vat']" position="after">
                <field name="masav"/>
                <field name="is_validation_masav" invisible="1"/>
                <field name="Masav_salary"/>
                <field name="masav_no" invisible="1"/>
                <field name="masav_no_ids">
                    <list editable="bottom">
                        <field name="masav_no" string="Masav No"/>
                        <field name="company_id" string=' ' invisible="1"/>
                    </list>
                </field>
            </xpath>
        </field>
    </record>

    <record id="view_partner_property_form_bizz" model="ir.ui.view">
        <field name="name">view.res.company.partner</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="account.view_partner_property_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='bank_ids']//field[@name='acc_number']"
                   position="after">
                <field name="branch_bank"/>
            </xpath>
        </field>
    </record>

    <record id="view_company_partner_bank_form_inherit" model="ir.ui.view">
        <field name="name">view.res.company.partner.form</field>
        <field name="model">res.partner.bank</field>
        <field name="inherit_id"
               ref="account.view_partner_bank_form_inherit_account"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='acc_number']" position="after">
                <field name="branch_bank"/>
            </xpath>
        </field>
    </record>

    <record id="view_partner_bank_form_inherit" model="ir.ui.view">
        <field name="name">view.res.company.partner.form</field>
        <field name="model">res.partner.bank</field>
        <field name="inherit_id" ref="base.view_partner_bank_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='acc_number']" position="after">
                <field name="branch_bank"/>
            </xpath>
        </field>
    </record>
</odoo>