<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="bizzup_pos_session.Navbar" t-inherit="point_of_sale.Navbar"
       t-inherit-mode="extension">
        <xpath expr="//Dropdown//div[contains(@class, 'pos-burger-menu-items')]"
               position="inside">
            <DropdownItem t-if="getPrintReportPosSession"
                          onSelected="() => this.printReportPosSession()">
                Print Report
            </DropdownItem>
        </xpath>
    </t>
</templates>
