<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_partner_form_inherit_for_moveing" model="ir.ui.view">
        <field name="name">res.partner.form.inherit.for.moveing</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='vat']" position="after">
                <field name="receipt_debit"/>
                <field name="receipt_credit"/>
            </xpath>
        </field>
    </record>
</odoo>
