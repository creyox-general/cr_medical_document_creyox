<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--  Inherit Event Res Config Settings Form View  -->
    <record model="ir.ui.view"
            id="res_config_settings_form_bizzup_movein_report_view">
        <field name="name">res.config.settings.form.bizzup.movein.report
        </field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="account.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//block[@id='analytic']"
                   position="inside">
                <div class="col-12 col-lg-6 o_setting_box" id="bizzup_movein_report_rivhit">
                    <div class="o_setting_left_pane">
                        <field name="is_for_rivhit"/>
                    </div>
                    <div class="o_setting_right_pane">
                        <label for="is_for_rivhit"/>
                        <div class="text-muted">
                            Print Movin Report for Rivhit
                        </div>
                    </div>
                </div>
                <div class="col-12 col-lg-6 o_setting_box" id="bizzup_movein_report_hrp">
                    <div class="o_setting_left_pane">
                        <field name="is_for_hrp"/>
                    </div>
                    <div class="o_setting_right_pane">
                        <label for="is_for_hrp"/>
                        <div class="text-muted">
                            Print Movein Report for Rivhit
                        </div>
                    </div>
                </div>
            </xpath>
        </field>
    </record>
</odoo>
