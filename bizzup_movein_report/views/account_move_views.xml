<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="account_move_form_inherit_for_moveing" model="ir.ui.view">
        <field name="name">account.move.form.inherit.for.moveing</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='partner_shipping_id']" position="after">
                <field groups="base.group_erp_manager" name="is_for_hrp" invisible="1"/>
                <field groups="base.group_erp_manager" name="is_pos_entry" invisible="1"/>
                <field groups="base.group_erp_manager" name="movement_code" invisible="is_for_hrp == True"/>
                <field groups="base.group_erp_manager" name="movement_code_string" invisible="is_for_hrp != True"/>
            </xpath>
        </field>
    </record>
</odoo>
