<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="account_account_form_inherit_for_moveing" model="ir.ui.view">
        <field name="name">account.account.form.inherit.for.moveing</field>
        <field name="model">account.account</field>
        <field name="inherit_id" ref="account.view_account_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='account_type']" position="after">
                <field name="external_software_code"/>
            </xpath>
        </field>
    </record>
</odoo>
